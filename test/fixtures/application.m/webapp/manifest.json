{
    "_version": "1.22.0",
    "sap.app": {
        "id": "application.m",
        "type": "application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "title": "{{title}}"
    },
    "sap.ui5": {
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "application.m.i18n.i18n"
                }
            },
            "i18n-ui5": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleUrl": "ui5://application/m/i18n/i18n.properties"
                }
            }
        }
    }
}